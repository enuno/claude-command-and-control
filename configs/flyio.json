{
  "name": "flyio",
  "description": "Fly.io platform for deploying full-stack apps and databases globally",
  "base_url": "https://fly.io/",
  "start_urls": [
    "https://fly.io/docs/getting-started/launch/",
    "https://fly.io/docs/apps/",
    "https://fly.io/docs/reference/configuration/"
  ],
  "url_patterns": {
    "include": [
      "/docs/getting-started/",
      "/docs/apps/",
      "/docs/reference/",
      "/docs/postgres/",
      "/docs/redis/"
    ],
    "exclude": [
      "/blog/",
      "/changelog/",
      "/about/",
      "/pricing/"
    ]
  },
  "categories": {
    "getting_started": ["getting-started", "launch", "quickstart"],
    "apps": ["apps", "deploy"],
    "databases": ["postgres", "redis", "database"],
    "configuration": ["reference", "configuration", "config"],
    "networking": ["networking", "network", "private-network"]
  },
  "rate_limit": 0.5,
  "max_pages": 100
}

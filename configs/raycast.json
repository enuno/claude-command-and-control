    ],
    "api": [
      "api-reference",
      "command",
      "browser",
      "cache",
      "clipboard",
      "environment",
      "keyboard",
      "oauth",
      "storage",
      "preferences",
      "window"
    ],
    "ui_components": [
      "list",
      "action-panel",
      "actions",
      "form",
      "detail",
      "grid",
      "colors",
      "icons",
      "navigation",
      "alert",
